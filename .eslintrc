{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-multiple-empty-lines": ["error", {"max": 1}],
    "space-before-function-paren": 0,
    "react/jsx-fragments": 0,
    "import/export": 0,
    "jsx-quotes": ["error", "prefer-double"],
    "quotes": [
      "error",
      "single",
      { "allowTemplateLiterals": true, "avoidEscape": true }
    ]
  }
}